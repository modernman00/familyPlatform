{
    "cSpell.words": [
        "PHPSESSID"
    ],
    "liveServer.settings.port": 5500,
    // This means that you change your real URL (current PHP url) 
    // to another URL (which Live Sever starts).
    "liveServer.settings.proxy": {
        "enable": true, //   i. enabled
        "baseUri": "/", //  ii. workspace
        "proxyUri": "http://localhost:80/" // iii. actual address
    },
    // vscode coloring
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "php.version": "8.4.5",
    "sqltools.connections": [
        {
            "mysqlOptions": {
                "authProtocol": "default",
                "enableSsl": "Disabled"
            },
            "previewLimit": 50,
            "server": "localhost",
            "port": 3306,
            "driver": "MySQL",
            "name": "mysql",
            "group": "family",
            "database": "family",
            "username": "root"
        }
    ],
    "http.systemCertificates": false,
    "intelephense.environment.phpVersion": "8.4.5"
}